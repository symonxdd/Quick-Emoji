            <CheckBox x:Name="chkMinimized" Content="Minimized" HorizontalAlignment="Left" VerticalAlignment="Top" Margin="219,89,0,0" Foreground="White" FontSize="10" Unchecked="chkMinimized_Unchecked" Checked="chkMinimized_Checked"/>
