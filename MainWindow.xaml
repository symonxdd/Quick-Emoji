        <Image x:Name="image8" HorizontalAlignment="Left" Height="34" VerticalAlignment="Top" Width="34" Source="Images/SmilingFaceWithHeart-ShapedEyes.png" Margin="44,35,0,0" MouseDown="image8_MouseDown"/>
