        <Image x:Name="image13" HorizontalAlignment="Left" Height="36" VerticalAlignment="Top" Width="36" Source="Images/ReversedHandWithMiddleFingerExtended.png" Margin="255,35,0,0" MouseDown="image13_MouseDown"/>
