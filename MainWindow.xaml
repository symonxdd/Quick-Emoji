        <Grid HorizontalAlignment="Left" Height="10" VerticalAlignment="Top" Width="83" Background="#FF151515" Margin="103,17,0,0" OpacityMask="Black"/>
        <Grid HorizontalAlignment="Left" Height="6" VerticalAlignment="Top" Width="73" Background="#FF191919" Margin="108,11,0,0"/>
