        <Label x:Name="label" Content="Quick Emoji" HorizontalAlignment="Left" VerticalAlignment="Top" Foreground="#FFFFC83D" FontSize="14" Margin="100,2,0,0" FontWeight="Bold"/>
        <Grid x:Name="grdSecretGrid" HorizontalAlignment="Left" Height="24" VerticalAlignment="Top" Width="83" Margin="103,7,0,0" MouseDown="grdSecretGrid_MouseDown" Background="#00000000"/>

    </Grid>
</Window>
